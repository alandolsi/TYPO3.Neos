wireframeMode = TYPO3.Neos.NodeTypes:Page
wireframeMode {
	headerData {
		stylesheets = TYPO3.TypoScript:Template
		stylesheets {
			templatePath = 'resource://TYPO3.Neos/Private/Templates/WireframeMode/Page/Default.html'
			sectionName = 'headerIncludes'
		}
	}

	bodyAttributes.class = 't3-wireframeMode'

	body {
		templatePath = 'resource://TYPO3.Neos/Private/Templates/WireframeMode/Page/Default.html'
		sectionName = 'body'

		parts {
			menu = TYPO3.Neos:Menu
			menu {
				templatePath = 'resource://TYPO3.Neos/Private/Templates/WireframeMode/TypoScriptObjects/Menu.html'
				entryLevel = 1
				maximumLevels = 4
			}
			breadcrumb = TYPO3.Neos:Breadcrumb
			breadcrumb.templatePath = 'resource://TYPO3.Neos/Private/Templates/WireframeMode/TypoScriptObjects/BreadcrumbMenu.html'
		}

		sections.all = TYPO3.Neos.NodeTypes:MultiColumn
		sections.all {
			templatePath = 'resource://TYPO3.Neos/Private/Templates/WireframeMode/TypoScriptObjects/GeneralSectionRendering.html'
			layout = ''
			columns {
				collection = ${q(node).children('[instanceof TYPO3.Neos.NodeTypes:Section]')}
				itemRenderer {
					templatePath = 'resource://TYPO3.Neos/Private/Templates/WireframeMode/TypoScriptObjects/SectionTemplate.html'
				}
			}
		}
	}
}