#
# If the headline starts with "Docume", prefix it with "DOCS" (globally)
#

namespace: default = TYPO3\TYPO3\TypoScript

prototype(TYPO3.TypoScript:Case).matchers.20 {
	condition = ${context.is('[headline ^= Docume]')}
	type = 'TYPO3.TypoScript:DocumentationText'
}
prototype(TYPO3.TypoScript:DocumentationText) < prototype(TYPO3.TypoScript:TextWithHeadline)
prototype(TYPO3.TypoScript:DocumentationText).headline << 1.wrap(prefix: 'DOCS: ')